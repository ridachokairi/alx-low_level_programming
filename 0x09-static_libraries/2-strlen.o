#include "main.h"

/**
 * _strlen - returns the length of a string
 * @s: the input  string
 * Return: the length of a the string s
 *
 */

int _strlen(char *s)
{
        int len = 0;

        while (*s != '\0')
        {
                len++;
                s++;
        }
        return (len);
}
