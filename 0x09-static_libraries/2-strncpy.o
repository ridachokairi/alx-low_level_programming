#include "main.h"

/**
 * strncpy - copies a string
 * @dest: the output string
 * @src: the copied string
 * @n: number of bytes of the copied string
 * Return: string
 */

char *_strncpy(char *dest, char *src, int n)
{
        int i = 0;

        while (i < n && src[i] != '\0')
        {
                dest[i] = src[i];
                i++;
        }
        while (i < n)
        {
                dest[i] = '\0';
                i++;
        }
        return (dest);
}
